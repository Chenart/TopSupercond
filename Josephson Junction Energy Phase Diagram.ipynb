    "# Pauli matrices\n",
    "s0 = tinyarray.array([[1, 0], [0, 1]])\n",
    "sx = tinyarray.array([[0, 1], [1, 0]])\n",
    "sy = tinyarray.array([[0, -1j], [1j, 0]])\n",
