    "def compute_energies(sys):\n",
    "    #Compute energies\n",
    "    ham_mat = sys.finalized().hamiltonian_submatrix(sparse=True)\n",
    "    ev = sla.eigsh(ham_mat, k=8, which='SM', return_eigenvectors=False, maxiter = 1000000)\n",
    "    \n",
    "    return ev\n"
   ]
